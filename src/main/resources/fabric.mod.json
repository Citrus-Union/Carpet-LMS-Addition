{
  "schemaVersion": 1,
  "id": "${id}",
  "version": "${version}",
  "name": "${name}",
  "description": "${description}",
  "authors": ["jasonxue", "stupidOIO", "小柠檬lemon酱"],
  "contact": {
    "homepage": "${website}",
    "issues": "${issues}",
    "sources": "${source}"
  },
  "license": "GPL-3.0-only",
  "icon": "assets/carpetlmsaddition/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": ["cn.nm.lms.carpetlmsaddition.CarpetLMSAdditionMod"]
  },
  "mixins": ["carpet-lms-addition.mixins.json"],
  "depends": {
    "fabricloader": "${fabricloader_dependency}",
    "minecraft": "${minecraft_dependency}",
    "fabric-api": "*",
    "carpet": "*"
  },
  "custom": {
    "modmenu": {
      "parent": "carpet",
      "links": {
        "modmenu.website": "${website}",
        "modmenu.modrinth": "${modrinth}",
        "modmenu.curseforge": "${curseforge}",
        "modmenu.github_releases": "${source}/releases",
        "modmenu.issues": "${issues}"
      },
      "update_checker": true
    }
  }
}
